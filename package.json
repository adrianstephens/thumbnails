{
	"name": "thumbnails",
	"displayName": "Thumbnail Icons",
	"description": "File Icons are thumbnails of their files",
	"keywords": [],
	"icon": "assets/thumb.png",
	"version": "0.0.0",
	"publisher": "isopodlabs",
	"repository": "https://github.com/adrianstephens/thumbnails",
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"iconThemes": [
			{
				"id": "thumbnails",
				"label": "Thumbnail File Icons",
				"path": "./icons/thumbnails.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -watch -p ./",
		"publish": "vsce publish patch"
	},
	"devDependencies": {
		"@types/node": "^18",
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^7.14.0",
		"@typescript-eslint/parser": "^7.14.0",
		"eslint": "^8.26.0",
		"ts-loader": "^9.5.1",
		"webpack": "^5.95.0",
		"webpack-cli": "^5.1.4"
	},
	"dependencies": {
		"sharp": "^0.33.5"
	}
